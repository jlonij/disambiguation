<Files web.py>                                                                                                                                                                                                                    
    SetHandler wsgi-script                                                                                                                                                                                                        
    Options +ExecCGI                                                                                                                                                                                                              
</Files>                                                                                                                                                                                                                          
                                                                                                                                                                                                                                  
RewriteEngine on                                                                                                                                                                                                                  
RewriteBase /dac
RewriteCond %{REQUEST_URI} !^/robots.txt                                                                                                                                                                                          
RewriteCond %{REQUEST_URI} !^/static                                                                                                                                                                                              
RewriteCond %{REQUEST_URI} !^(/.*)+web.py                                                                                                                                                                                         
RewriteRule ^(.*)$ web.py/$1 [PT] 
